{
  "name": "server",
  "version": "0.1.23",
  "description": "Sol server",
  "author": "Sol",
  "private": true,
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RedPlanetHQ/sol.git"
  },
  "bugs": {
    "url": "https://github.com/RedPlanetHQ/sol.git/issues"
  },
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start-prod": "pnpm run migrate:deploy && pnpm run generate && node dist/src/main",
    "start-prod-with-prisma": "pnpm run start-prod && pnpm prisma:studio",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "migrate:resolve": "prisma migrate resolve --preview-feature",
    "prisma:studio": "prisma studio",
    "generate": "prisma generate",
    "db:init-resources": "pnpm run migrate && pnpm run generate",
    "trigger-deploy": "npx trigger.dev@3.3.17 deploy --self-hosted",
    "trigger-dev": "npx trigger.dev@3.3.17 dev"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@ai-sdk/google": "^1.2.16",
    "@ai-sdk/openai": "^1.3.21",
    "@anthropic-ai/claude-code": "1.0.30",
    "@anthropic-ai/sdk": "^0.18.0",
    "@aws-sdk/client-s3": "^3.712.0",
    "@aws-sdk/s3-request-presigner": "^3.712.0",
    "@google-cloud/storage": "^7.10.0",
    "@hocuspocus/extension-database": "^2.13.7",
    "@hocuspocus/server": "^2.13.7",
    "@hocuspocus/transformer": "^2.13.7",
    "@modelcontextprotocol/sdk": "1.11.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "11.1.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/platform-ws": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.0",
    "@paciolan/remote-module-loader": "^3.0.3",
    "@prisma/client": "^6.7.0",
    "@prisma/instrumentation": "^6.7.0",
    "@qdrant/js-client-rest": "^1.14.0",
    "@sol/editor-extensions": "workspace:*",
    "@redplanethq/sol-sdk": "0.2.15",
    "@trigger.dev/sdk": "3.3.17",
    "@types/multer": "^1.4.11",
    "@types/turndown": "^5.0.5",
    "@xenova/transformers": "^2.16.1",
    "ai": "4.3.14",
    "axios": "^1.6.7",
    "bcrypt": "5.1.1",
    "cache-manager": "^6.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cohere-ai": "^7.17.1",
    "cookie": "^1.0.2",
    "date-fns": "^3.6.0",
    "date-fns-tz": "3.2.0",
    "execa": "^9.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.1.0",
    "marked": "^14.1.2",
    "nest-winston": "^1.9.7",
    "nestjs-prisma": "0.22.0",
    "nodemailer": "^7.0.2",
    "pg": "^8.15.6",
    "pg-logical-replication": "^2.0.7",
    "prisma": "5.21.0",
    "prosemirror-model": "^1.20.0",
    "reflect-metadata": "^0.2.1",
    "rrule": "^2.8.1",
    "rxjs": "7.8.1",
    "simple-oauth2": "^5.1.0",
    "socket.io": "^4.8.1",
    "supertokens-node": "^21.1.1",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "y-prosemirror": "^1.3.4",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.26",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.0",
    "@sol/eslint-config": "workspace:*",
    "@sol/typescript-config": "workspace:*",
    "@trigger.dev/build": "3.3.17",
    "@types/bcrypt": "5.0.2",
    "@types/chance": "1.1.6",
    "@types/cookie": "^0.6.0",
    "@types/express": "5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.14",
    "@types/pg": "^8.15.0",
    "@types/simple-oauth2": "^5.0.7",
    "@types/supertest": "6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^8.56.0",
    "jest": "29.7.0",
    "libphonenumber-js": "^1.11.7",
    "prettier": "^3.5.3",
    "prisma-dbml-generator": "0.12.0",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.3.0"
}
